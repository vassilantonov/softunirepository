using System;

namespace baba
{
    class ti
    {
        static void Main()
        {
            int x = int.Parse(Console.ReadLine());
            var arrej = new int[x];
            var m = 0;
            for (int i = 0; i < arrej.Length; i++)
            {
                arrej[i] =int.Parse(Console.ReadLine());
                if (arrej[i]<0)
                {
                    m++;
                }
            }
            Console.WriteLine(m);
        }
    }
}
