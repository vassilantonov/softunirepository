using System;
using System.Linq;

namespace baba
{
    class ti
    {
        static void Main()
        {
            double[] arr = Console.ReadLine().Split(' ').Select(double.Parse).ToArray();
            double x = double.Parse(Console.ReadLine());
            int p = 0;
            for (int i = 0; i < arr.Length; i++)
            {
                if (arr[i]>x)
                {
                    p++;
                }
            }
            Console.WriteLine(p);
        }
    }
}
