using System;
using System.Linq;

namespace baba
{
    class ti
    {
        static void Main()
        {
            int[] arr = Console.ReadLine().Split(' ').Select(int.Parse).ToArray();
            int x = int.Parse(Console.ReadLine());
            int p = 0;
            for (int i = 0; i < arr.Length; i++)
            {
                if (arr[i]==x)
                {
                    p++;
                }
            }
            Console.WriteLine(p);
        }
    }
}
